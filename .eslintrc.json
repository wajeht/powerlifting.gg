{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"overrides": [],
	"ignorePatterns": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"globals": {
		"logger": "readonly"
	},
	"rules": {
		"no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
	}
}
